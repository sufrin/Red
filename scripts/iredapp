# Behave (as far as possible) like the packaged app (for testing)
# (avoids the need to re-package the app while testing)
CP=/Users/sufrin/local/scalalib/scala-swing.jar:/Users/sufrin/GitHomes/RegexKit/out/artifacts/regexkit.jar:./out/production/Jed
D="-DJed.UDPServer.level=ALL -DRed.AppleRed.level=ALL"
echo $D
env REDPORT=3344 \
    JAVACMD=java \
    JAVA_OPTS="-Dapplered.app=true -Xdock:name=iRedApp -Xdock:icon=/Users/sufrin/bin/cred.png" \
    scala $D -cp $CP Red.AppleRed "$@" &
