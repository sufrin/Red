# Client-server invocation of Red
# First invocation becomes the server 
# Subsequent invocations act as clients, delegating to the server
REDPORT=33445 exec java \
     -Xdock:icon=/Users/sufrin/bin/red.png  \
     -Xdock:name="AppleRed-Client&Server" \
     -DLogging.Default.level=error \
     -jar ~/bin/AppleRed.jar "$@" &

